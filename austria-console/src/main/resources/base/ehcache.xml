<?xml version="1.0" encoding="UTF-8"?>

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd">

    <defaultCache
            maxEntriesLocalHeap="10000"
            eternal="false"
            timeToIdleSeconds="10"
            timeToLiveSeconds="10"
            memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_1s"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="1"
           timeToLiveSeconds="1"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_3s"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="3"
           timeToLiveSeconds="3"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_5s"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="5"
           timeToLiveSeconds="5"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_10s"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="10"
           timeToLiveSeconds="10"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_30s"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="30"
           timeToLiveSeconds="30"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_1m"
           maxEntriesLocalHeap="5000"
           eternal="false"
           timeToIdleSeconds="60"
           timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_3m"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="180"
           timeToLiveSeconds="180"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="cache_10m"
           maxEntriesLocalHeap="10000"
           eternal="false"
           timeToIdleSeconds="600"
           timeToLiveSeconds="600"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="open_apps"
           maxEntriesLocalHeap="256"
           eternal="false"
           timeToIdleSeconds="86400"
           timeToLiveSeconds="86400"
           memoryStoreEvictionPolicy="LRU"/>

    <cache name="partner"
           maxEntriesLocalHeap="256"
           eternal="false"
           timeToIdleSeconds="86400"
           timeToLiveSeconds="86400"
           memoryStoreEvictionPolicy="LRU"/>

</ehcache>
